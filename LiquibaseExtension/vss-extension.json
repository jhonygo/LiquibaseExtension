{
  "manifestVersion": 1,
  "id": "liquibase-extension",
  "version": "0.1.0",
  "name": "Liquibase Extension",
  "scopes": [ "vso.work", "vso.code", "vso.build", "vso.packaging" ],
  "description": "Liquibase extension for TFS",
  "overview" : "Liquibase is a framework aimed to manage database changes in a tracked and reproducible manner.  For details see [Liquibase homepage](http://www.liquibase.org).",
  "publisher": "sedatguclu",
  "public": false,
  "icons": {
    "default": "img/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Build, Release"
  ],
  "links": {
  },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "files": [
    {
      "path": "css",
      "addressable": true
    },
    {
      "path": "img",
      "addressable": true
    },
    {
      "path": "scripts",
      "addressable": true
    },
    {
      "path": "taskVersion",
      "addressable" : true
    },
    {
      "path": "index.html",
      "addressable": true
    }
  ],
  "categories": [
    "Integrate"
  ],
  "contributions": [
    {
      "id": "taskVersion",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "taskVersion"
      }
    }
  ]
}
