{
  "manifestVersion": 1,
  "id": "liquibase-extension",
  "name": "Liquibase Extension",
  "version": "0.1.6",
  "publisher": "sedatifpcdeboiron",
  "description": "Liquibase extension for TFS",
  "overview" : "Liquibase is a framework aimed to manage database changes in a tracked and reproducible manner.  For details see [Liquibase homepage](http://www.liquibase.org).",
  "public": false,
  "icons": {
    "default": "img/logo.png"
  },
  "screenshots": [ ],
   "categories": [
    "Build and release"
  ],
  "tags": [
    "utility", "tasks"
  ],
  "links": {
      "Source code": {
        "uri" : "https://github.com/jhonygo/LiquibaseExtension" }
    },
  "branding": {
    "color": "rgb(220, 235, 252)",
    "theme": "light"
  },
  "content": {
    "details": {
      "path": "LiquibaseExtension/readme.md"
    }
  },
  "files": [
    {
      "path": "tasks"
    }
  ],
  "scopes": [],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "contributions": [
    {
      "id": "task-liquibase-execute",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "LiquibaseExtension/tasks/TaskLiquibaseExecute"
      }
    }
  ]
}
